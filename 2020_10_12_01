/*
Count pairs of nums in an array whose sum is divisble by 4
*/

#include <bits/stdc++.h>
using namespace std;

int solve(vector<int> arr) {
    int count[] = {0, 0, 0, 0};
    for(int num : arr) count[num%4]++;
    return count[0]*(count[0]-1)/2
            + count[2]*(count[2]-1)/2
            + count[1]*count[3];
}

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);
    for(int i=0; i<n; i++) cin>>arr[i];
    cout << solve(arr) << endl;
    return 0;
}
